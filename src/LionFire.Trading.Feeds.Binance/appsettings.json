{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "TradingFeeds": {
    "Storage": {
      "LogDirectory": "./data/binance",
      "SegmentSize": 1073741824,
      "MemorySize": 268435456,
      "PageSizeBits": 22
    }
  },
  "BinanceFeed": {
    "Symbols": [
      "BTCUSDT",
      "ETHUSDT"
    ],
    "CollectTrades": true,
    "CollectOrderBook": true,
    "CollectOnTradeOnly": true,
    "OrderBookDepth": {
      "Collect01Percent": true,
      "Collect025Percent": true,
      "Collect05Percent": true,
      "Collect075Percent": true,
      "Collect1Percent": true,
      "Collect2Percent": true
    },
    "ApiKey": "",
    "ApiSecret": "",
    "UseTestnet": false
  }
}